{
  "name": "apex-api-express",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sls offline --stage offline",
    "tail-log": "sls logs -f app -t",
    "deploy-prod": "cross-env NODE_ENV=production sls deploy --stage prod",
    "remove-prod": "sls remove --stage prod",
    "deploy-development": "cross-env NODE_ENV=development SLS_DEBUG=true sls deploy --stage dev",
    "remove-development": "sls remove --stage dev",
    "clean": "rimraf dist .webpack .serverless",
    "lint": "eslint --ext .js,.ts .",
    "prepare": "husky install"
  },
  "author": "Ketan Somani",
  "license": "MIT",
  "dependencies": {
    "aws-sdk": "2.1122.0",
    "babel-runtime": "^6.26.0",
    "cognito-express": "3.0.1",
    "express": "4.18.1",
    "express-validator": "6.14.0",
    "helmet": "5.0.2",
    "http-status": "^1.5.1",
    "mongoose": "6.3.0",
    "mongoose-delete": "0.5.4",
    "mongoose-paginate-v2": "1.6.3",
    "serverless-http": "2.7.0",
    "source-map-support": "^0.4.18"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-source-map-support": "^1.0.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-3": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "cross-env": "7.0.3",
    "eslint": "8.13.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-unused-imports": "2.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.7",
    "prettier": "2.6.2",
    "rimraf": "3.0.2",
    "serverless": "2.72.3",
    "serverless-dotenv-plugin": "3.12.2",
    "serverless-offline": "8.5.0",
    "serverless-webpack": "^5.7.0",
    "webpack": "^4.16.2",
    "webpack-node-externals": "^3.0.0"
  }
